{
  "name": "astrogif",
  "author": "James Macfie <james@macfie.co.nz> (https://github.com/jamesmacfie)",
  "version": "0.4.0",
  "description": "A menubar app for gif searches",
  "main": "index.js",
  "scripts": {
    "hot-server": "node -r babel-register webpack/devServer.js",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack/webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack/webpack.config.production.js --progress --profile --colors",
    "build": "yarn build-main && yarn build-renderer",
    "start": "cross-env NODE_ENV=production electron ./",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register ./main.js",
    "dev": "concurrently --kill-others \"yarn hot-server\" \"yarn start-hot\"",
    "release": "yarn build && build -mwl --publish always",
    "release-mac": "yarn build && build -m",
    "release-windows": "yarn build && build -w",
    "release-linux": "yarn build && build -l",
    "release-linux-deb": "yarn build && build -l deb",
    "release-linux-rpm": "yarn build && build -l rpm",
    "release-linux-pacman": "yarn build && build -l pacman",
    "test-renderer": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 electron-mocha --renderer --compilers js:babel-register --recursive --require ./test/setup.js app/**/*.test.js",
    "test-main": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 electron-mocha --compilers js:babel-register --recursive --require ./test/setup.js",
    "test": "yarn test-main && yarn test-renderer"
  },
  "build": {
    "productName": "AstroGif",
    "appId": "com.jamesmacfie.astro",
    "directories": {
      "output": "release"
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/astrogif/astrogif.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/astrogif/astrogif/issues"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "css-modules-require-hook": "^4.0.5",
    "electron-config": "^0.2.1",
    "electron-devtools-installer": "^2.0.1",
    "fuzzy": "^0.1.3",
    "menubar": "6.0.3",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-shortcut-chooser": "^1.1.2",
    "redux-logger": "^2.6.1",
    "redux-saga": "^0.14.3",
    "request": "^2.79.0",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.9",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-webpack-loaders": "^0.8.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-properties": "^1.2.1",
    "concurrently": "^2.2.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "electron": "^9.4.0",
    "electron-builder": "^20.43.0",
    "electron-mocha": "^8.0.2",
    "electron-packager": "^13.1.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-import-resolver-webpack": "^0.7.1",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.8.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "ignore-styles": "^5.0.1",
    "image-webpack-loader": "^3.1.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "react-addons-test-utils": "^15.4.2",
    "react-redux": "^4.4.5",
    "react-test-utils": "0.0.1",
    "redux": "^3.5.2",
    "redux-mock-store": "^1.2.2",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.1",
    "svg-sprite-loader": "^0.2.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  }
}
