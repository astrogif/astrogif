{
  "name": "example-menubar-app",
  "version": "1.0.0",
  "description": "example menubar electron app",
  "dependencies": {
    "classnames": "^2.2.5",
    "css-modules-require-hook": "^4.0.5",
    "electron-config": "^0.2.1",
    "electron-devtools-installer": "^2.0.1",
    "lodash.debounce": "^4.0.8",
    "menubar": "*",
    "react": "^15.4.1",
    "react-autobind": "^1.0.6",
    "react-dom": "^15.4.1",
    "request": "^2.79.0"
  },
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.9",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-webpack-loaders": "^0.8.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "concurrently": "^2.2.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "electron": "^1.4.12",
    "electron-builder": "^10.8.1",
    "electron-packager": "^5.1.0",
    "electron-prebuilt": "^0.36.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-import-resolver-webpack": "^0.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "image-webpack-loader": "^3.1.0",
    "json-loader": "^0.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  },
  "main": "main.js",
  "scripts": {
    "hot-server": "node -r babel-register server.js",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "start": "cross-env NODE_ENV=production electron ./",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register ./main/index.js",
    "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\"",
    "release": "build -mwl --publish always",
    "release-mac": "npm run build && build -m",
    "release-win": "npm run build && build -w"
  }
}
